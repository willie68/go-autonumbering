    "version": "0.2.0",
    "configurations": [
        {
            "name": "autonumbering",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "H:/temp/011"
            ]
        },
        {
            "name": "revert autonumbering",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "-r",
                "H:/temp/011"
            ]
        }
    ]
}